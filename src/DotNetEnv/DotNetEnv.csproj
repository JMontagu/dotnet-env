<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Title>DotNetEnv</Title>
    <Description>A .NET library to load environment variables from .env</Description>
    <AssemblyTitle>DotNetEnv</AssemblyTitle>
    <PackageVersion>1.1.0</PackageVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>DotNetEnv</AssemblyName>
    <OutputType>Library</OutputType>
    <PackageId>DotNetEnv</PackageId>
    <PackageTags>dotnet;env;dotfiles</PackageTags>
    <PackageProjectUrl>https://github.com/tonerdo/dotnet-env</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/tonerdo/dotnet-env/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/tonerdo/dotnet-env</RepositoryUrl>
  </PropertyGroup>
</Project>
