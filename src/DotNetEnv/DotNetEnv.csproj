<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>A library to load .env file into Environment variable</Description>
    <AssemblyTitle>DotNetEnv</AssemblyTitle>
    <VersionPrefix>1.0.1</VersionPrefix>
    <Authors>Toni Solarin-Sodara</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>DotNetEnv</AssemblyName>
    <OutputType>Library</OutputType>
    <PackageId>DotNetEnv</PackageId>
    <PackageTags>dotnet;env;dotfiles</PackageTags>
    <PackageProjectUrl>https://github.com/tsolarin/dotnet-env</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/tsolarin/dotnet-env/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>
</Project>
